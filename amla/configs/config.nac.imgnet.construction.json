{
"parameters": {
        "arch_name":"nac-imgnet-cons",
        "exec":"localhost",
        "mode":"construct",
        "algorithm": "envelopenet",
        "gpus":[],
        "gpu_usage":0.47,
        "steps" : 20000,
        "eval_interval" : 10000,
        "batch_size" : 128,
        "dataset":"imagenet",
        "data_dir":"../../../../data/imagenet-tfrecord",
        "iterations" : 6,
        "image_size" : 224,
        "log_stats": true
},
"child_training": {
    "optimizer": {"type": "momentum", "momentum": 0.9},
    "lr": {"type":"exponential_decay", "initial":0.1},
    "commentgradient_clipping": {"type": "norm", "value":5.0},
    "commentregularization": {"type": "l2", "value": 3e-4}
},
"envelopenet":{ "stages": 4,
        "layers_per_stage": [2,2,2,2],
        "max_layers_per_stage": [7,6,4,3],
        "construction": [true, true, true, true],
        "parameter_limits": [false, false, false, false],
        "max_filter_prune":6,
        "envelopecell": {
            "Branch1": "1x1",
            "Branch2": "3x3",
            "Branch3": "3x3sep",
            "Branch4": "5x5",
            "Branch5": "5x5sep",
            "Branch6": "7x7sep"
        },
        "skip": true,
        "worst_case": false
},
"init_cell": {
     "Layer1": {"Branch0": { "block": "conv2d", "kernel_size": [3,3], "outputs": 32, "bias": false, "stride": 2}},
     "Layer2": {"Branch0": { "block": "conv2d", "kernel_size": [3,3], "outputs": 64, "bias": false, "stride": 2}},
     "Layer3": {"Branch0": { "block": "conv2d", "kernel_size": [3, 3], "outputs": 64, "bias": false, "stride": 2}}
},
"classification_cell": {
    "Layer0": {"Branch0": {"block": "reduce_mean", "size": [1, 2]}},
    "Layer1": {"Branch0": {"block": "flatten", "size": [3, 3]}},
    "Layer2": {"Branch0": {"block": "dropout", "keep_prob": 0.8}},
    "Layer3": {"Branch0": {"block": "fc-final", "inputs": 192, "outputs": 1001}}
}
}
